	<artifactId>jeelite-data-provider</artifactId>

	<dependencies>
		<dependency>
			<groupId>com.github.flying</groupId>
			<artifactId>jeelite-common</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
